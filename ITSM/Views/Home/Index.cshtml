@using ITSM_DomainModelEntity.ViewModels
@model AllModelVM
@{
    ViewData["Title"] = "Home Page";
    Layout = "_Layout";
}

<style>
    .dropdown-item.year-item.active {
    background-color: #696cff;
    color: #fff;
    }
</style>

<nav class="layout-navbar container-xxl navbar-detached navbar navbar-expand-xl align-items-center bg-navbar-theme"
id="layout-navbar">
    <div class="layout-menu-toggle navbar-nav align-items-xl-center me-4 me-xl-0 d-xl-none">
        <a class="nav-item nav-link px-0 me-xl-6" href="javascript:void(0)">
            <i class="icon-base bx bx-menu icon-md"></i>
        </a>
    </div>

    <div class="navbar-nav-right d-flex align-items-center justify-content-end" id="navbar-collapse">
        <ul class="navbar-nav flex-row align-items-center ms-md-auto">
            <!-- User -->
            <li class="nav-item navbar-dropdown dropdown-user dropdown open-button">
                <a class="nav-link dropdown-toggle hide-arrow p-0"
                href="javascript:void(0);"
                data-bs-toggle="dropdown">
                    <div class="avatar avatar-online">
                        <img src="@(Model.user.photo != null ? $"data:{Model.user.photo_type};base64," +  Convert.ToBase64String(Model.user.photo) :
                            Url.Content("~/img/avatar/user_avatar.jpg"))" alt="avatar" class="w-px-40 rounded-circle" />
                    </div>
                </a>
            </li>
            <!--/ User -->
        </ul>
    </div>
</nav>

<!-- / Navbar -->
<!-- Content wrapper -->
<div class="content-wrapper">
    <!-- Content -->
    <div class="container-xxl flex-grow-1 container-p-y">
        <div class="row">

            <!-- Home -->
            <div class="col-xxl-8 mb-6 order-0">
                <div class="card">
                    <div class="d-flex align-items-start row">
                        <div class="col-sm-7">
                            <div class="card-body">
                                <h5 class="card-title text-primary mb-3">Welcome back @(Model.user.fullname)! 🎉</h5>
                                <p class="mb-6">
                                    Hope you're having a great day at work.<br />"Don't forget to take a break and grab some lunch!"
                                </p>

                                <a href="~/Personal/Home" class="btn btn-sm btn-outline-primary">View Personal Information</a>
                            </div>
                        </div>
                        <div class="col-sm-5 text-center text-sm-left">
                            <div class="card-body pb-0 px-0 px-md-6">
                                <img src="~/assets/img/illustrations/man-with-laptop.png"
                                height="175"
                                alt="View Badge User" />
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!--/ Home -->

            <!-- box -->
            <div class="col-xxl-4 col-lg-12 col-md-4 order-1">
                <div class="row">
                    <div class="col-lg-6 col-md-12 col-6 mb-6">
                        <div class="card h-100">
                            <div class="card-body">
                                <div class="card-title d-flex align-items-start justify-content-between mb-4">
                                    <div class="avatar flex-shrink-0">
                                        <img src="~/assets/img/icons/unicons/chart-success.png"
                                        alt="chart success"
                                        class="rounded" />
                                    </div>
                                    <div class="dropdown">
                                        <button class="btn p-0"
                                        type="button"
                                        id="cardOpt3"
                                        data-bs-toggle="dropdown"
                                        aria-haspopup="true"
                                        aria-expanded="false">
                                            <i class="icon-base bx bx-dots-vertical-rounded text-body-secondary"></i>
                                        </button>
                                        <div class="dropdown-menu dropdown-menu-end" aria-labelledby="cardOpt3">
                                            <a class="dropdown-item infor-today" href="javascript:void(0);">
                                                <svg width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
                                                    <circle cx="12" cy="12" r="10"></circle>
                                                    <path d="M9.09 9a3 3 0 0 1 5.83 1c0 2-3 3-3 3"></path>
                                                    <line x1="12" y1="17" x2="12.01" y2="17"></line>
                                                </svg>
                                                Information
                                            </a>
                                        </div>
                                    </div>
                                </div>
                                <p class="mb-1">Incidents 
                                    <span class="badge bg-label-info" style="font-size:0.5rem;">
                                        TODAY
                                    </span>
                                </p>
                                <h4 class="card-title mb-3">@Model.today_inc_count</h4>
                                @if (Model.today_inc_percent != 0)
                                {
                                    <small class="text-success fw-medium"><i class="icon-base bx bx-up-arrow-alt"></i> @(Model.today_inc_percent)%</small>
                                }
                                else
                                {
                                    <small class="text-success fw-medium">-</small>
                                }
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-6 col-md-12 col-6 mb-6">
                        <div class="card h-100">
                            <div class="card-body">
                                <div class="card-title d-flex align-items-start justify-content-between mb-4">
                                    <div class="avatar flex-shrink-0">
                                        <img src="~/assets/img/icons/unicons/wallet-info.png"
                                        alt="wallet info"
                                        class="rounded" />
                                    </div>
                                    <div class="dropdown">
                                        <button class="btn p-0"
                                        type="button"
                                        id="cardOpt6"
                                        data-bs-toggle="dropdown"
                                        aria-haspopup="true"
                                        aria-expanded="false">
                                            <i class="icon-base bx bx-dots-vertical-rounded text-body-secondary"></i>
                                        </button>
                                        <div class="dropdown-menu dropdown-menu-end" aria-labelledby="cardOpt6">
                                            <a class="dropdown-item infor-today" href="javascript:void(0);">
                                                <svg width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
                                                    <circle cx="12" cy="12" r="10"></circle>
                                                    <path d="M9.09 9a3 3 0 0 1 5.83 1c0 2-3 3-3 3"></path>
                                                    <line x1="12" y1="17" x2="12.01" y2="17"></line>
                                                </svg>
                                                Information
                                            </a>
                                        </div>
                                    </div>
                                </div>
                                <p class="mb-1">Requests 
                                    <span class="badge bg-label-info" style="font-size:0.5rem;">
                                        TODAY
                                    </span>
                                </p>
                                <h4 class="card-title mb-3">@Model.today_req_count</h4>
                                @if (Model.today_req_percent != 0)
                                {
                                    <small class="text-success fw-medium"><i class="icon-base bx bx-up-arrow-alt"></i> @(Model.today_req_percent)%</small>
                                }
                                else
                                {
                                    <small class="text-success fw-medium">-</small>
                                }
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!--/ box -->

            <!-- Analysis Incidents -->
            <div class="col-12 col-xxl-8 order-2 order-md-3 order-xxl-2 mb-6 total-revenue">
                <div class="card">
                    <div class="row row-bordered g-0">
                        <div class="col-lg-8">
                            <div class="card-header d-flex align-items-center justify-content-between">
                                <div class="card-title mb-0">
                                    <h5 class="m-0 me-2">Analysis Incidents</h5>
                                </div>

                                @if (Model.user.Role.role.ToLower() == "admin")
                                {
                                    <div class="dropdown">
                                        <button class="btn text-body-secondary p-0"
                                                type="button"
                                                id="orederStatistics"
                                                data-bs-toggle="dropdown"
                                                aria-haspopup="true"
                                                aria-expanded="false">
                                            <i class="icon-base bx bx-dots-vertical-rounded icon-lg"></i>
                                        </button>
                                        <div class="dropdown-menu dropdown-menu-end" aria-labelledby="orederStatistics">
                                            <a class="dropdown-item" href="~/IncidentManagement/All">View More</a>
                                        </div>
                                    </div>
                                }
                               
                            </div>
                            <div id="totalRevenueChart" class="px-3"></div>
                            <div id="incidentStatsData" data-stats='@Json.Serialize(Model.YearlyIncidentStats)' style="display: none;"></div>
                        </div>
                        <div class="col-lg-4">
                            <div class="card-body px-xl-9 py-12 d-flex align-items-center flex-column">
                                <div class="text-center mb-6">
                                    <div class="btn-group">
                                        <button type="button" class="btn btn-outline-primary">
                                            @DateTime.Now.Year
                                        </button>
                                        <button type="button"
                                                class="btn btn-outline-primary dropdown-toggle dropdown-toggle-split"
                                                data-bs-toggle="dropdown"
                                                aria-expanded="false">
                                            <span class="visually-hidden">Toggle Dropdown</span>
                                        </button>
                                        <ul class="dropdown-menu">
                                            @foreach (var yearStat in Model.YearlyIncidentStats.OrderByDescending(x => x.Year))
                                            {
                                                <li><a class="dropdown-item year-item" href="javascript:void(0);" data-year="@yearStat.Year">@yearStat.Year</a></li>
                                            }
                                        </ul>
                                    </div>
                                </div>

                                <div id="growthChart"></div>
                                <div class="text-center fw-medium my-6">@Model.YearlyIncidentStats.FirstOrDefault(x => x.Year == DateTime.Now.Year)?.Growth.ToString("F1")% Resolved Rate</div>

                                <div class="d-flex gap-11 justify-content-between">
                                    <div class="d-flex">
                                        <div class="avatar me-2">
                                            <span class="avatar-initial rounded-2 bg-label-primary">
                                                <i class="icon-base bx bx-wallet icon-lg text-primary"></i>
                                            </span>
                                        </div>
                                        <div class="d-flex flex-column">
                                            <small class="prev-year">
                                                @(DateTime.Now.Year - 1)
                                            </small>
                                            <h6 class="mb-0 growth-value">@Model.YearlyIncidentStats.FirstOrDefault(x => x.Year == DateTime.Now.Year - 1)?.Growth.ToString("F1")%</h6>
                                        </div>
                                    </div>
                                    <div class="d-flex">
                                        <div class="avatar me-2">
                                            <span class="avatar-initial rounded-2 bg-label-info">
                                                <i class="icon-base bx bx-wallet icon-lg text-info"></i>
                                            </span>
                                        </div>
                                        <div class="d-flex flex-column">
                                            <small class="next-year">
                                                @(DateTime.Now.Year - 2)
                                            </small>
                                            <h6 class="mb-0 growth-value">@Model.YearlyIncidentStats.FirstOrDefault(x => x.Year == DateTime.Now.Year - 2)?.Growth.ToString("F1")%</h6>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!--/ Analysis Incidents -->

            <!-- box -->
            <div class="col-12 col-md-8 col-lg-12 col-xxl-4 order-3 order-md-2 profile-report">
                <div class="row">
                    <div class="col-6 mb-6 payments">
                        <div class="card h-100">
                            <div class="card-body">
                                <div class="card-title d-flex align-items-start justify-content-between mb-4">
                                    <div class="avatar flex-shrink-0">
                                        <img src="~/assets/img/icons/unicons/paypal.png" alt="paypal" class="rounded" />
                                    </div>
                                    <div class="dropdown">
                                        <button class="btn p-0"
                                                type="button"
                                                id="cardOpt4"
                                                data-bs-toggle="dropdown"
                                                aria-haspopup="true"
                                                aria-expanded="false">
                                            <i class="icon-base bx bx-dots-vertical-rounded text-body-secondary"></i>
                                        </button>
                                        <div class="dropdown-menu dropdown-menu-end" aria-labelledby="cardOpt4">
                                            <a class="dropdown-item infor-today" href="javascript:void(0);">
                                                <svg width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
                                                    <circle cx="12" cy="12" r="10"></circle>
                                                    <path d="M9.09 9a3 3 0 0 1 5.83 1c0 2-3 3-3 3"></path>
                                                    <line x1="12" y1="17" x2="12.01" y2="17"></line>
                                                </svg>
                                                Information
                                            </a>
                                        </div>
                                    </div>
                                </div>
                                <p class="mb-1">Feedbacks 
                                    <span class="badge bg-label-info" style="font-size:0.5rem;">
                                        TODAY
                                    </span>
                                </p>
                                <h4 class="card-title mb-3">@Model.today_fd_count</h4>
                                @if (Model.today_fd_percent != 0)
                                {
                                    <small class="text-success fw-medium"><i class="icon-base bx bx-up-arrow-alt"></i> @(Model.today_fd_percent)%</small>
                                }
                                else
                                {
                                    <small class="text-success fw-medium">-</small>
                                }
                            </div>
                        </div>
                    </div>
                    <div class="col-6 mb-6 transactions">
                        <div class="card h-100">
                            <div class="card-body">
                                <div class="card-title d-flex align-items-start justify-content-between mb-4">
                                    <div class="avatar flex-shrink-0">
                                        <img src="~/assets/img/icons/unicons/cc-primary.png" alt="Credit Card" class="rounded" />
                                    </div>
                                    <div class="dropdown">
                                        <button class="btn p-0"
                                                type="button"
                                                id="cardOpt1"
                                                data-bs-toggle="dropdown"
                                                aria-haspopup="true"
                                                aria-expanded="false">
                                            <i class="icon-base bx bx-dots-vertical-rounded text-body-secondary"></i>
                                        </button>
                                        <div class="dropdown-menu" aria-labelledby="cardOpt1">
                                            <a class="dropdown-item infor-today" href="javascript:void(0);">
                                                <svg width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
                                                    <circle cx="12" cy="12" r="10"></circle>
                                                    <path d="M9.09 9a3 3 0 0 1 5.83 1c0 2-3 3-3 3"></path>
                                                    <line x1="12" y1="17" x2="12.01" y2="17"></line>
                                                </svg>
                                                Information
                                            </a>
                                        </div>
                                    </div>
                                </div>
                                <p class="mb-1">Knowledges 
                                    <span class="badge bg-label-info" style="font-size:0.5rem;">
                                        TODAY
                                    </span>
                                </p>
                                <h4 class="card-title mb-3">@Model.today_kb_count</h4>
                                @if (Model.today_kb_percent != 0)
                                {
                                    <small class="text-success fw-medium"><i class="icon-base bx bx-up-arrow-alt"></i> @(Model.today_kb_percent)%</small>
                                }
                                else
                                {
                                    <small class="text-success fw-medium">-</small>
                                }
                            </div>
                        </div>
                    </div>
                    <div class="col-12 mb-6 profile-report">
                        <div class="card h-100">
                            <div class="card-body">
                                <div class="d-flex justify-content-between align-items-center flex-sm-row flex-column gap-10 flex-wrap">
                                    <div class="d-flex flex-sm-column flex-row align-items-start justify-content-between">
                                        <div class="card-title mb-6">
                                            <h5 class="text-nowrap mb-1">Todo</h5>
                                            <span class="badge bg-label-warning">
                                                YEAR
                                                <script>
                                                    document.write(new Date().getFullYear());
                                                </script>
                                            </span>
                                        </div>
                                        <div class="mt-sm-auto">

                                            @if (Model.todo_d_count > 0)
                                            {
                                                <span class="text-success text-nowrap fw-medium">
                                                    <i class="icon-base bx bx-up-arrow-alt"></i> @($"{Model.todo_d_count:F2}%")
                                                </span>
                                            }
                                            else
                                            {
                                                <span class="text-success text-nowrap fw-medium">
                                                    - @($"{Model.todo_d_count:F2}%")
                                                </span>
                                            }

                                            <h4 class="mb-0">@Model.todo_count</h4>
                                        </div>
                                    </div>
                                    <div id="profileReportChart"></div>
                                    <div id="todoStatsDataMonth" data-stats='@Json.Serialize(Model.MonthlyTodoStats)' style="display: none;"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!--/ box -->

        </div>
        <div class="row">

            <!-- Total Statistics -->
            <div class="col-md-6 col-lg-4 col-xl-4 order-0 mb-6">
                <div class="card h-100">
                    <div class="card-header d-flex justify-content-between">
                        <div class="card-title mb-0">
                            <h5 class="mb-1 me-2">Total Statistics (Personal)</h5>
                            <p class="card-subtitle">
                                <span class="badge bg-label-info" style="font-size:0.6rem;">
                                    THIS WEEK DATA
                                </span>
                            </p>
                        </div>
                        <div class="dropdown">
                            <button class="btn text-body-secondary p-0"
                                    type="button"
                                    id="orederStatistics"
                                    data-bs-toggle="dropdown"
                                    aria-haspopup="true"
                                    aria-expanded="false">
                                <i class="icon-base bx bx-dots-vertical-rounded icon-lg"></i>
                            </button>
                            <div class="dropdown-menu dropdown-menu-end" aria-labelledby="orederStatistics">
                                <a class="dropdown-item" href="~/Personal/Home">View More</a>
                            </div>
                        </div>
                    </div>
                    <div class="card-body">
                        <div class="d-flex justify-content-between align-items-center mb-6">
                            <div class="d-flex flex-column align-items-center gap-1">
                                <h3 class="mb-1">@Model.total_count</h3>
                                <small>Total Count</small>
                            </div>
                            <div id="chart-data"
                                 data-incident="@Model.inc_count"
                                 data-request="@Model.req_count"
                                 data-feedback="@Model.fd_count"
                                 data-knowledge="@Model.kb_count"
                                 data-total="@Model.total_count">
                            </div>
                            <div id="orderStatisticsChart"></div>
                        </div>
                        <ul class="p-0 m-0">
                            <li class="d-flex align-items-center mb-5">
                                <div class="avatar flex-shrink-0 me-3">
                                    <span class="avatar-initial rounded bg-label-primary">
                                        <i class="icon-base bx bx-mobile-alt"></i>
                                    </span>
                                </div>
                                <div class="d-flex w-100 flex-wrap align-items-center justify-content-between gap-2">
                                    <div class="me-2">
                                        <h6 class="mb-0">Incident</h6>
                                        <small>Incidents calculation</small>
                                    </div>
                                    <div class="user-progress">
                                        <h6 class="mb-0">@Model.inc_count</h6>
                                    </div>
                                </div>
                            </li>
                            <li class="d-flex align-items-center mb-5">
                                <div class="avatar flex-shrink-0 me-3">
                                    <span class="avatar-initial rounded bg-label-success">
                                        <i class="icon-base bx bx-closet"></i>
                                    </span>
                                </div>
                                <div class="d-flex w-100 flex-wrap align-items-center justify-content-between gap-2">
                                    <div class="me-2">
                                        <h6 class="mb-0">Requests</h6>
                                        <small>Requests calculation</small>
                                    </div>
                                    <div class="user-progress">
                                        <h6 class="mb-0">@Model.req_count</h6>
                                    </div>
                                </div>
                            </li>
                            <li class="d-flex align-items-center mb-5">
                                <div class="avatar flex-shrink-0 me-3">
                                    <span class="avatar-initial rounded bg-label-info">
                                        <i class="icon-base bx bx-home-alt"></i>
                                    </span>
                                </div>
                                <div class="d-flex w-100 flex-wrap align-items-center justify-content-between gap-2">
                                    <div class="me-2">
                                        <h6 class="mb-0">Feedbacks</h6>
                                        <small>Feedbacks calculation</small>
                                    </div>
                                    <div class="user-progress">
                                        <h6 class="mb-0">@Model.fd_count</h6>
                                    </div>
                                </div>
                            </li>
                            <li class="d-flex align-items-center">
                                <div class="avatar flex-shrink-0 me-3">
                                    <span class="avatar-initial rounded bg-label-secondary">
                                        <i class="icon-base bx bx-football"></i>
                                    </span>
                                </div>
                                <div class="d-flex w-100 flex-wrap align-items-center justify-content-between gap-2">
                                    <div class="me-2">
                                        <h6 class="mb-0">Knowledges</h6>
                                        <small>Knowledges calculation</small>
                                    </div>
                                    <div class="user-progress">
                                        <h6 class="mb-0">@Model.kb_count</h6>
                                    </div>
                                </div>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
            <!--/ Total Statistics -->

            <!-- Expense Overview -->
            <div class="col-md-6 col-lg-4 order-1 mb-6">
                <div class="card h-100">
                    <div class="card-header nav-align-top">
                        <h5 class="mb-1 me-2">Analysis Requests</h5>
                    </div>
                    <div class="card-body">
                        <div class="tab-content p-0">
                            <div class="tab-pane fade show active" id="navs-tabs-line-card-income" role="tabpanel">
                                <div class="d-flex mb-6">
                                    <div class="avatar flex-shrink-0 me-3">
                                        <span class="avatar-initial rounded-2 bg-label-primary">
                                            <i class="fa-solid fa-chart-simple"></i>
                                        </span>
                                    </div>
                                    <div>
                                        <p class="mb-0">
                                            <span class="badge bg-label-warning">
                                                YEAR
                                                <script>
                                                    document.write(new Date().getFullYear());
                                                </script>
                                            </span>
                                        </p>
                                        <div class="d-flex align-items-center">
                                            <h6 class="mb-0 me-1">@Model.yearly_req_total</h6>

                                            @if (Model.yearly_req_completed_percent > 0){
                                                <small class="text-success fw-medium">
                                                    <i class="icon-base bx bx-chevron-up icon-lg"></i>
                                                    @($"{Model.yearly_req_completed_percent:F2}%")
                                                </small>
                                            }else{
                                                <small class="text-success fw-medium">
                                                    - 
                                                    @($"{Model.yearly_req_completed_percent:F2}%")
                                                </small>
                                            }
                                            
                                        </div>
                                    </div>
                                </div>
                                <div id="incomeChart"></div>
                                <div id="requestChartStatsData" data-stats='@Json.Serialize(Model.MonthlyRequestStats)' style="display: none;"></div>
                                <div class="d-flex align-items-center justify-content-center mt-6 gap-3">
                                    <div class="flex-shrink-0">
                                        <div id="expensesOfWeek"></div>
                                        <div id="requestOtherStateStatsData" data-stats='@Json.Serialize(Model.yearly_req_other_percent)' style="display: none;"></div>
                                    </div>
                                    <div>
                                        <h6 class="mb-0">Other state for this year</h6>
                                        <small>
                                            @Model.yearly_req_other_count still processing
                                        </small>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!--/ Expense Overview -->

            <!-- Announcement -->
            <div class="col-md-6 col-lg-4 order-2 mb-6">
                <div class="card h-100">
                    <div class="card-header d-flex align-items-center justify-content-between">
                        <h5 class="card-title m-0 me-2">Announcement</h5>
                        <div class="dropdown">
                            <button class="btn text-body-secondary p-0"
                                    type="button"
                                    id="transactionID"
                                    data-bs-toggle="dropdown"
                                    aria-haspopup="true"
                                    aria-expanded="false">
